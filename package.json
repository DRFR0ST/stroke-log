{
	"name": "stroke-log",
	"displayName": "Stroke Log",
	"description": "Counts all keystrokes.",
	"author": "Mike Eling <mike.eling97@gmail.com>",
	"publisher": "drfrost",
	"repository": "https://github.com/DRFR0ST/stroke-log",
	"version": "1.1.1",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"main": "./extension.js",
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#C80000",
		"theme": "dark"
	},
