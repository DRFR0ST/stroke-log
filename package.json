{
	"name": "strokey",
	"displayName": "Strokey",
	"description": "Counts all your keystrokes.",
	"author": "Mike Eling <mike.eling97@gmail.com>",
	"publisher": "drfrost",
	"repository": "https://github.com/DRFR0ST/strokey",
	"version": "1.4.0",
	"license": "MIT",
	"icon": "images/icon.png",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:strokey.toggleWorkspace",
		"onCommand:strokey.setGoal",
		"onCommand:strokey.showProgress",
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "strokey.toggleWorkspace",
				"title": "Toggle workspace log",
				"category": "Strokey",
				"icon": "$(keyboard)"
			},
			{
				"command": "strokey.setGoal",
				"title": "Set daily goal",
				"category": "Strokey",
				"icon": "$(keyboard)"
			},
			{
				"command": "strokey.showProgress",
				"title": "Show todays progress",
				"category": "Strokey",
				"icon": "$(keyboard)"
			},
			{
				"command": "strokey.signin",
				"title": "Sign-in",
				"category": "Strokey",
				"icon": "$(keyboard)"
			},
			{
				"command": "strokey.signout",
				"title": "Sign-out",
				"category": "Strokey",
				"icon": "$(keyboard)"
			},
			{
				"command": "strokey.signup",
				"title": "Sign-up",
				"category": "Strokey",
				"icon": "$(keyboard)"
			},
			{
				"command": "strokey.token",
				"title": "Show personal token",
				"category": "Strokey",
				"icon": "$(keyboard)"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/request": "^2.48.4",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"galleryBanner": {
		"color": "#C80000",
		"theme": "dark"
	},
	"dependencies": {
		"request": "^2.88.2"
	}
}
